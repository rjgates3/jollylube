{
    "version": 2,
    "alias": "jollylube",
    "name": "jollylube",
    "routes": [
      {
          "src": "^/static/(.*)",
          "dest": "/static/$1"
      },
      {
          "src": ".*",
          "dest": "./index.html"
      }
    ],
    "builds":[
        {
            "src": "^/favicon.ico$",
            "dest": "/favicon.ico",
            "use": "@now/static"
           }
    ],
    "env": {
        "DB_URL": "https://cryptic-brook-57150.herokuapp.com/"
    }
}