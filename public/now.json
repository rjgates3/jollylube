{
    "version": 2,
    "alias": "jollylube",
    "name": "jollylube",
    "routes": [
      {
          "src": "^/static/(.*)",
          "dest": "/static/$1"
      },
      {
          "src": ".*",
          "dest": "./index.html"
      },
      {
        "src": "^/favicon.ico$",
        "dest": "/favicon.ico"
       }
    ],
    "env": {
        "DB_URL": "https://cryptic-brook-57150.herokuapp.com/"
    }
}